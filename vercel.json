{
  "version": 2,
  "builds": [
    {
      "src": "src/server/vercel/api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "npm run build",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/server/vercel/api/index.js"
    },
    {
      "src": "^/$",
      "dest": "src/server/vercel/api/index.js"
    },
    {
      "src": "/demo",
      "dest": "/dist/demo.html"
    }
  ]
}
